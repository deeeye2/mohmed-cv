# Temporarily remove .env from .gitignore
sed -i.bak '/.env/d' .gitignore

# Add .env back to Git tracking
git add .env
git commit -m "Unhide .env file"
git push origin master

# Restore .env to .gitignore
echo ".env" >> .gitignore

# Remove .env from Git tracking again
git rm --cached .env
git commit -m "Hide .env file again"
git push origin master

